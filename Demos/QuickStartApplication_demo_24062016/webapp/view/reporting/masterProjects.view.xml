<mvc:View
    id="inicioHojaDeGastos"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	controllerName="QuickStartApplication.controller.reporting.masterProjects"
	xmlns="sap.m">
	<SplitApp id="SplitAppDemo" initialMaster="masterProjects">
		<detailPages>
                <!--<mvc:XMLView id="vistaHojaGastos" viewName="QuickStartApplication.view.view.hojaDeGastos"/>-->
		</detailPages>
		<masterPages>
			<Page id="masterProjects" title="My projects" icon="sap-icon://action" class="sapUiStdPage"
			    showNavButton="true" navButtonPress="onNavDashboard">
				<content>
                    <SearchField id="buscadorProyecto" width="100%" placeholder="Search" liveChange="onSearch"/>
					<List
					    id="listProjects"
						items="{
							path: 'projects>/Projects'
						}" >
						<StandardListItem
							title="{projects>Nombre}"
							description="{projects>Codigo}"
							type="Navigation" 
							press="onPressProject"/>
					</List>

				</content>
			</Page>
		</masterPages>
	</SplitApp>
</mvc:View>