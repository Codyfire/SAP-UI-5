			                        <IconTabFilter text="Shipped" count="5" icon="sap-icon://shipping-status" iconColor="Positive"/></items>
			                </IconTabHeader>
			                <IconTabBar selectedKey="__filter3" id="__bar0">
			                    <items>
			                        <IconTabFilter text="Orders" count="22" showAll="true" id="__filter3">
			                            <content>
			                                <List noDataText="Sin datos" id="__list2">
