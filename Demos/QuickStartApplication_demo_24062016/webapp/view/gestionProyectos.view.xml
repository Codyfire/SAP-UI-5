			                    </l:content>
			                </l:Grid>
			            </commons:firstPaneContent>
			        <commons:secondPaneContent>
			            <NavContainer id="__container0">
			            <pages>
			                <IconTabHeader id="__header0">
			                    <items>
			                        <IconTabFilter text="Orders" textDirection="LTR" count="22" showAll="true">
			                            <content>
			                                <List noDataText="Sin datos">
			                                    <customData>
			                                        <core:CustomData key="sap-ui-fastnavgroup" value="true" writeToDom="true" id="__data8"/>
